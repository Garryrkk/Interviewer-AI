{
  "name": "agenda-frontend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "electron-main.js",
  "scripts": {
    "dev": "vite",
    "start:electron": "concurrently -k \"npm:dev\" \"wait-on http://localhost:0 && electron .\"",
    "build": "vite build",
    "electron:build": "electron-builder",
    "serve": "vite preview",
    "lint": "eslint . --ext .js,.jsx"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "cross-env": "^10.0.0",
    "framer-motion": "^11.2.10",
    "lucide-react": "^0.542.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.3",
    "react-router-dom": "^6.23.1",
    "react-speech-recognition": "^3.10.0",
    "react-webcam": "^7.1.4",
    "zustand": "^4.5.5"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.14",
    "concurrently": "^9.2.1",
    "electron": "^38.0.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.10.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "postcss": "^8.4.24",
    "tailwindcss": "^3.3.2",
    "vite": "^4.5.14",
    "wait-on": "^8.0.4"
  },
  "build": {
    "appId": "com.youragency.agenda",
    "productName": "Agenda AI",
    "files": [
      "dist/**/*",
      "electron-main.js",
      "preload-main.js",
      "preload-overlay.js",
      "overlay.html",
      "package.json"
    ],
    "mac": {
      "category": "public.app-category.productivity"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "description": "",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
